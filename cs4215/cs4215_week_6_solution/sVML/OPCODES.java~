package cs3212.sVMLheap;

public class OPCODES {
  public static final byte
      AND      =  0,
      CALL     =  1,
      DIV      =  2,
      DONE     =  3,
      EQUAL    =  4,
      GOTO     =  5,
      GREATER  =  6,
      JOF      =  7,
      LD       =  8,
      LDCB     =  9,
      LDCI     = 10,
      LDF      = 11,
      LDRF     = 12,
      LESS     = 13,
      MINUS    = 14,
      NOT      = 15,
      OR       = 16,
      PLUS     = 17,
      RTN      = 18,
      TIMES    = 19,
      TAILCALL = 20,
      START    = 21;
}
